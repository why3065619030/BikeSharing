<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".history">
    <LinearLayout
        android:layout_width="409dp"
        android:layout_height="729dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp"
        tools:ignore="MissingConstraints">

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="速度变化图" />
        <com.example.bikesharing.notuse.MiuiWeatherView

            android:id="@+id/weather"
            android:layout_width="match_parent"

            android:layout_height="300dp"

            app:line_interval="60dp"

            app:min_point_height="60dp"

            app:background_color="#ffffff"
            tools:ignore="MissingClass,MissingConstraints"></com.example.bikesharing.notuse.MiuiWeatherView>



    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>